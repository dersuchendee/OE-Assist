@prefix : <http://w3id.org/CEON/ontology/AllCombined/> .
@prefix cvn: <http://w3id.org/CEON/ontology/cvn/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix actor: <http://w3id.org/CEON/ontology/actor/> .
@prefix process: <http://w3id.org/CEON/ontology/process/> .
@prefix actorODP: <http://w3id.org/CEON/ontology/actorODP/> .
@prefix material: <http://w3id.org/CEON/ontology/material/> .
@prefix processODP: <http://w3id.org/CEON/ontology/processODP/> .
@prefix resourceODP: <http://w3id.org/CEON/ontology/resourceODP/> .
@base <http://w3id.org/CEON/ontology/AllCombined0.1/> .

<http://w3id.org/CEON/ontology/ont4/> rdf:type owl:Ontology.
#AllCombined
#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/contributor
<http://purl.org/dc/terms/contributor> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements
<http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gMonthYear
xsd:gMonthYear rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/CEON/ontology/actor/capabilityExtent
actor:capabilityExtent rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf actorODP:capabilityProperty ;
                       rdfs:domain actorODP:Capability ;
                       rdfs:comment "The extent of this capability, e.g. stating whether there are limist to the capability, such as a maximum amount or size of something." ;
                       rdfs:label "capability extent" .


###  http://w3id.org/CEON/ontology/actor/neededResourceRelation
actor:neededResourceRelation rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf actorODP:capabilityProperty ;
                             rdfs:domain actorODP:Capability ;
                             rdfs:comment "In order for an actor to have a capability, it needs to have certain resources, e.g. materials, infrastrucutre, know-how, information etc." ;
                             rdfs:label "needed resource relations" .


###  http://w3id.org/CEON/ontology/actorODP/actorType
actorODP:actorType rdf:type owl:ObjectProperty ;
                   rdfs:domain actorODP:Actor ;
                   rdfs:range actorODP:ActorType ;
                   rdfs:comment "The type of the actor." ;
                   rdfs:label "has actor type" .


###  http://w3id.org/CEON/ontology/actorODP/basedOnCapability
actorODP:basedOnCapability rdf:type owl:ObjectProperty ;
                           rdfs:domain actorODP:CollaborationParticipation ;
                           rdfs:range actorODP:Capability ;
                           rdfs:comment "An actor participates in a collaboration based on that it has some capability that is useful for the collaboration." ;
                           rdfs:label "based on capability" .


###  http://w3id.org/CEON/ontology/actorODP/capabilityOf
actorODP:capabilityOf rdf:type owl:ObjectProperty ;
                      rdfs:domain actorODP:Capability ;
                      rdfs:range actorODP:Actor ;
                      rdfs:label "Relates to the actor holding the capability." ,
                                 "capability of" .


###  http://w3id.org/CEON/ontology/actorODP/capabilityProperty
actorODP:capabilityProperty rdf:type owl:ObjectProperty ;
                            rdfs:domain actorODP:Capability ;
                            rdfs:comment "Relates some properties to the capability, such as the parameters of it, or the needed resources." ;
                            rdfs:label "capability property" .


###  http://w3id.org/CEON/ontology/actorODP/hasLocation
actorODP:hasLocation rdf:type owl:ObjectProperty ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( actorODP:Actor
                                                 resourceODP:Resource
                                               )
                                 ] .


###  http://w3id.org/CEON/ontology/actorODP/participantRole
actorODP:participantRole rdf:type owl:ObjectProperty ;
                         rdfs:domain actorODP:Participation ;
                         rdfs:range actorODP:Role ;
                         rdfs:comment "Holds the value of the role of the participant in this participation relation." ;
                         rdfs:label "has participant role" .


###  http://w3id.org/CEON/ontology/actorODP/participatingActor
actorODP:participatingActor rdf:type owl:ObjectProperty ;
                            rdfs:domain actorODP:Participation ;
                            rdfs:range actorODP:Actor ;
                            rdfs:comment "Holds the value of the actor involved in this participation relation." ;
                            rdfs:label "has participating actor" .


###  http://w3id.org/CEON/ontology/actorODP/participatingResource
actorODP:participatingResource rdf:type owl:ObjectProperty ;
                               rdfs:domain actorODP:ResourceRelation ;
                               rdfs:range resourceODP:Resource ;
                               rdfs:comment "The resource that this participation relation relates to, i.e. for which the actor holds the specified role." ;
                               rdfs:label "has participating resource" .


###  http://w3id.org/CEON/ontology/actorODP/participationIn
actorODP:participationIn rdf:type owl:ObjectProperty ;
                         rdfs:domain actorODP:CollaborationParticipation ;
                         rdfs:range [ rdf:type owl:Class ;
                                      owl:unionOf ( cvn:Collaboration
                                                    process:Process
                                                  )
                                    ] ;
                         rdfs:comment "The collaboration or process that this participation relates to." ;
                         rdfs:label "is a participation in" .


###  http://w3id.org/CEON/ontology/cvn/aimsAtValue
cvn:aimsAtValue rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( cvn:CVN
                                            cvn:CVNBlueprint
                                          )
                            ] ;
                rdfs:range <http://w3id.org/CEON/ontology/value/ValueProposition> ;
                rdfs:comment "The value proposition that the planned abstract CVN configuration, or concrete network wants to achieve." ;
                rdfs:label "aims at value" .


###  http://w3id.org/CEON/ontology/cvn/composedOf
cvn:composedOf rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( cvn:CVN
                                           processODP:Process
                                         )
                           ] ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( cvn:CVN
                                          processODP:Process
                                        )
                          ] ;
               rdfs:comment "Both a CVN and a process can be composed of other CVNs or processes." ;
               rdfs:label "composed of" .


###  http://w3id.org/CEON/ontology/cvn/createsValue
cvn:createsValue rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( actorODP:Participation
                                             cvn:CVN
                                             processODP:Process
                                           )
                             ] ;
                 rdfs:range <http://w3id.org/CEON/ontology/value/Value> ;
                 rdfs:comment "Value creation can be captured at the actor level, i.e. value created by an actor's participation in a collaboration, or at the process or complete CVN level." ;
                 rdfs:label "creates value" .


###  http://w3id.org/CEON/ontology/cvn/implementsBlueprint
cvn:implementsBlueprint rdf:type owl:ObjectProperty ;
                        rdfs:domain cvn:CVN ;
                        rdfs:range cvn:CVNBlueprint ;
                        rdfs:comment "The blueprint (or plan) that this concrete CVN is an instance of." ;
                        rdfs:label "implements blueprint" .


###  http://w3id.org/CEON/ontology/cvn/implementsStrategy
cvn:implementsStrategy rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf cvn:relatedStrategy ;
                       rdfs:domain cvn:CVN ;
                       rdfs:range cvn:CircularStrategy ;
                       rdfs:comment "A strategy that is implemented by this CVN or CVN blueprint." ;
                       rdfs:label "implements strategy" .


###  http://w3id.org/CEON/ontology/cvn/plansToImplementStrategy
cvn:plansToImplementStrategy rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf cvn:relatedStrategy ;
                             rdfs:domain cvn:CVNBlueprint ;
                             rdfs:range cvn:CircularStrategy ;
                             rdfs:comment "A strategy that is planned to be implemented by this CVN or CVN blueprint." ;
                             rdfs:label "plans to implement strategy" .


###  http://w3id.org/CEON/ontology/cvn/relatedStrategy
cvn:relatedStrategy rdf:type owl:ObjectProperty ;
                    rdfs:comment "A strategy that is targeted by this CVN or CVN blueprint." ;
                    rdfs:label "related strategy" .


###  http://w3id.org/CEON/ontology/material/hasChemicalEntity
material:hasChemicalEntity rdf:type owl:ObjectProperty ;
                           rdfs:domain material:Material ;
                           rdfs:range material:ChemicalEntity ;
                           rdfs:comment "hasChemicalEntity intends to represent that a material can have a collection of chemical entities."@en ;
                           rdfs:label "has chemical entity"@en .


###  http://w3id.org/CEON/ontology/material/hasMaterialComponent
material:hasMaterialComponent rdf:type owl:ObjectProperty ;
                              rdfs:domain material:Material ;
                              rdfs:range material:MaterialComponent ;
                              rdfs:comment "hasMaterialComponent intends to represent that a material can have a collection of components."@en ;
                              rdfs:label "has material component"@en .


###  http://w3id.org/CEON/ontology/process/needsEnergy
process:needsEnergy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf processODP:hasInput ;
                    rdfs:range process:Energy ;
                    rdfs:comment "The energy needed to perform a certain process." ;
                    rdfs:label "needs energy" .


###  http://w3id.org/CEON/ontology/process/producesCO2
process:producesCO2 rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf processODP:hasOutput ;
                    rdfs:range process:CO2Emission ;
                    rdfs:comment "The CO2 released or produced by the process." ;
                    rdfs:label "produces CO2" .


###  http://w3id.org/CEON/ontology/process/resultingProduct
process:resultingProduct rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf processODP:hasOutput ;
                         rdfs:range resourceODP:Resource ;
                         rdfs:comment "The resource that is the output (product, i.e. what is produced) from a certain process." ;
                         rdfs:label "resulting product" .


###  http://w3id.org/CEON/ontology/process/usesCathalyst
process:usesCathalyst rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf processODP:hasInput ;
                      rdfs:range process:Cathalyst ;
                      rdfs:comment "The cathalyst used in a process." ;
                      rdfs:label "uses cathalyst" .


###  http://w3id.org/CEON/ontology/processODP/hasInput
processODP:hasInput rdf:type owl:ObjectProperty ;
                    rdfs:domain processODP:Transformation ;
                    rdfs:comment "The situation before the transformation takes place, i.e. the state of affairs before the process takes places, such as the set of components before they are assembled into a product." ;
                    rdfs:label "has input" .


###  http://w3id.org/CEON/ontology/processODP/hasOutput
processODP:hasOutput rdf:type owl:ObjectProperty ;
                     rdfs:domain processODP:Transformation ;
                     rdfs:comment "The output situation of a transformation, i.e. the state of affairs after the transformation took place." ;
                     rdfs:label "has output" .


###  http://w3id.org/CEON/ontology/processODP/hasPart
processODP:hasPart rdf:type owl:ObjectProperty ,
                            owl:TransitiveProperty ;
                   rdfs:domain processODP:Process ;
                   rdfs:range processODP:Process ;
                   rdfs:comment "A process can consists of several part, which are sub-processes." ;
                   rdfs:label "has part" .


###  http://w3id.org/CEON/ontology/processODP/hasProcessExecution
processODP:hasProcessExecution rdf:type owl:ObjectProperty ;
                               rdfs:domain processODP:Process ;
                               rdfs:range processODP:ProcessExecution ;
                               rdfs:comment "A process can have  specific executions."@en ;
                               rdfs:label "has process execution"@en .


###  http://w3id.org/CEON/ontology/processODP/hasProcessType
processODP:hasProcessType rdf:type owl:ObjectProperty ;
                          rdfs:domain processODP:Process ;
                          rdfs:range processODP:ProcessType ;
                          rdfs:comment "The type of the process."@en ;
                          rdfs:label "has process type"@en .


###  http://w3id.org/CEON/ontology/processODP/intialSituation
processODP:intialSituation rdf:type owl:ObjectProperty ;
                           rdfs:domain processODP:Transformation ;
                           rdfs:range processODP:Situation ;
                           rdfs:comment "The situation before executing a process." ;
                           rdfs:label "initial situation" .


###  http://w3id.org/CEON/ontology/processODP/isSettingFor
processODP:isSettingFor rdf:type owl:ObjectProperty ;
                        rdfs:domain processODP:Situation ;
                        rdfs:comment "The thing(s) that this situation is the setting for, e.g. a resource that is in a certain state." ;
                        rdfs:label "is the setting for" .


###  http://w3id.org/CEON/ontology/processODP/nextProcess
processODP:nextProcess rdf:type owl:ObjectProperty ;
                       rdfs:domain processODP:Process ;
                       rdfs:range processODP:Process ;
                       rdfs:comment "A relation between one process and the next one following it in some sequence of processes, e.g. a sequence of transformations in a material flow." ;
                       rdfs:label "next process" .


###  http://w3id.org/CEON/ontology/processODP/occursInLocation
processODP:occursInLocation rdf:type owl:ObjectProperty ;
                            rdfs:domain processODP:Transformation ;
                            rdfs:comment "The location in which a transformation takes place, e.g. the factory location where something is manufactured." ;
                            rdfs:label "occurs in location" .


###  http://w3id.org/CEON/ontology/processODP/resultingSituation
processODP:resultingSituation rdf:type owl:ObjectProperty ;
                              rdfs:domain processODP:Transformation ;
                              rdfs:range processODP:Situation ;
                              rdfs:comment "The situation after the execution of a process." ;
                              rdfs:label "resulting situation" .


###  http://w3id.org/CEON/ontology/product/hasProductComponent
<http://w3id.org/CEON/ontology/product/hasProductComponent> rdf:type owl:ObjectProperty ;
                                                            rdfs:domain <http://w3id.org/CEON/ontology/product/Product> ;
                                                            rdfs:range <http://w3id.org/CEON/ontology/product/ProductComponent> ;
                                                            rdfs:comment "hasProductComponent intends to represent that a product can have a collection of components."@en ;
                                                            rdfs:label "hasProductComponent"@en .


###  http://w3id.org/CEON/ontology/resourceODP/hasConstituent
resourceODP:hasConstituent rdf:type owl:ObjectProperty ;
                           rdfs:domain resourceODP:PhysicalObject ;
                           rdfs:range resourceODP:Constituent ;
                           rdfs:comment "hasConstitutent intends to represent that a physical object can have a collection of composing components."@en ;
                           rdfs:label "hasConstituent"@en .


###  http://w3id.org/CEON/ontology/resourceODP/hasMatter
resourceODP:hasMatter rdf:type owl:ObjectProperty ;
                      rdfs:domain resourceODP:PhysicalObject ;
                      rdfs:range resourceODP:Matter ;
                      rdfs:comment "hasMatter intends to represent that a physical object can have a collection of matter."@en ;
                      rdfs:label "hasMatter"@en .


#################################################################
#    Data properties
#################################################################

###  http://w3id.org/CEON/ontology/actorODP/participationEndTime
actorODP:participationEndTime rdf:type owl:DatatypeProperty ;
                              rdfs:range [ rdf:type rdfs:Datatype ;
                                           owl:unionOf ( xsd:date
                                                         xsd:dateTime
                                                         xsd:gMonthYear
                                                         xsd:gYear
                                                       )
                                         ] ;
                              rdfs:comment "The end of a time interval." ;
                              rdfs:label "end time" .


###  http://w3id.org/CEON/ontology/actorODP/participationStartTime
actorODP:participationStartTime rdf:type owl:DatatypeProperty ;
                                rdfs:domain actorODP:Participation ;
                                rdfs:range [ rdf:type rdfs:Datatype ;
                                             owl:unionOf ( xsd:date
                                                           xsd:dateTime
                                                           xsd:gMonthYear
                                                           xsd:gYear
                                                         )
                                           ] ;
                                rdfs:comment "The start of a time interval." ;
                                rdfs:label "start time" .


###  http://w3id.org/CEON/ontology/actorODP/participationTimePoint
actorODP:participationTimePoint rdf:type owl:DatatypeProperty ;
                                rdfs:domain actorODP:Participation ;
                                rdfs:range [ rdf:type rdfs:Datatype ;
                                             owl:unionOf ( xsd:date
                                                           xsd:dateTime
                                                           xsd:gMonthYear
                                                           xsd:gYear
                                                         )
                                           ] ;
                                rdfs:comment "The point in time when something took place or was valid." ;
                                rdfs:label "time point" .


###  http://w3id.org/CEON/ontology/material/AnonymousFormula
material:AnonymousFormula rdf:type owl:DatatypeProperty ;
                          rdfs:comment "AnonymousFormula represents that a molecular entity has the anonymous formula in a string."@en ;
                          rdfs:label "anonymous formula" ;
                          rdfs:seeAlso "https://w3id.org/mdo/structure/AnonymousFormula" .


###  http://w3id.org/CEON/ontology/material/DescriptiveFormula
material:DescriptiveFormula rdf:type owl:DatatypeProperty ;
                            rdfs:comment "DescriptiveFormula represents that a molecular entity has the descriptive formula in a string."@en ;
                            rdfs:label "descriptive formula" ;
                            rdfs:seeAlso "https://w3id.org/mdo/structure/DescriptiveFormula" .


###  http://w3id.org/CEON/ontology/material/HillFormula
material:HillFormula rdf:type owl:DatatypeProperty ;
                     rdfs:comment "HillFormula represents that a composition has the hill formula in a string."@en ;
                     rdfs:label "hill formula" ;
                     rdfs:seeAlso "https://w3id.org/mdo/structure/HillFormula" .


###  http://w3id.org/CEON/ontology/material/ReducedChemicalFormula
material:ReducedChemicalFormula rdf:type owl:DatatypeProperty ;
                                rdfs:comment "ReducedChemicalFormula represents that a molecular entity has the reduced chemical formula in a string."@en ;
                                rdfs:label "reduced formula" ;
                                rdfs:seeAlso "https://w3id.org/mdo/structure/ReducedFormula" .


#################################################################
#    Classes
#################################################################

###  http://w3id.org/CEON/ontology/actor/ActorCVNRole
actor:ActorCVNRole rdf:type owl:Class ;
                   rdfs:subClassOf actor:ActorCollaborationRole ;
                   rdfs:comment "The role(s) of an actor in a Circular Value Network, which is a specific type of collaboration." ;
                   rdfs:label "Actor Circular Value Network role" .


###  http://w3id.org/CEON/ontology/actor/ActorCollaborationRole
actor:ActorCollaborationRole rdf:type owl:Class ;
                             rdfs:subClassOf actorODP:Role ;
                             rdfs:comment "The roles of an actor involved in a collaboration." ;
                             rdfs:label "Actor collaboration role" .


###  http://w3id.org/CEON/ontology/actor/ActorProcessRole
actor:ActorProcessRole rdf:type owl:Class ;
                       rdfs:subClassOf actor:ActorCollaborationRole ;
                       rdfs:comment "The role(s) of an actor involved in a process." ;
                       rdfs:label "Actor process role" .


###  http://w3id.org/CEON/ontology/actor/ActorResourceRole
actor:ActorResourceRole rdf:type owl:Class ;
                        rdfs:subClassOf actorODP:Role ;
                        rdfs:comment "The role(s) of an actor in relation to a resource." ;
                        rdfs:label "Actor resource role" .


###  http://w3id.org/CEON/ontology/actor/BuyingResource
actor:BuyingResource rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( actorODP:ResourceRelation
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty actorODP:participantRole ;
                                                                  owl:hasValue actor:buyer
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf actorODP:ResourceRelation ;
                     rdfs:comment "The relation where the role of the actor is the buyer of a certain resource." ;
                     rdfs:label "Buying resource" .


###  http://w3id.org/CEON/ontology/actor/CVNParticipation
actor:CVNParticipation rdf:type owl:Class ;
                       rdfs:subClassOf actorODP:CollaborationParticipation ,
                                       [ owl:intersectionOf ( actorODP:CollaborationParticipation
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty actorODP:participantRole ;
                                                                owl:someValuesFrom actor:ActorCVNRole
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty actorODP:participatingActor ;
                                                                owl:someValuesFrom actorODP:Actor
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty actorODP:participationIn ;
                                                                owl:someValuesFrom cvn:CVN
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                       rdfs:comment "A participation-relation, that represents the participation of an actor in a CVN with a certain role. For instance, a specific company playing the role of recycler in a certain flow of a CVN. The participation may also be related to a certain time (or time interval)." ;
                       rdfs:label "Circular Value Network Participation" .


###  http://w3id.org/CEON/ontology/actor/ConsumingResource
actor:ConsumingResource rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( actorODP:ResourceRelation
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty actorODP:participantRole ;
                                                                     owl:hasValue actor:consumer
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf actorODP:ResourceRelation ;
                        rdfs:comment "The relation where the role of the actor is the consumer of a certain resource." ;
                        rdfs:label "Consuming resource" .


###  http://w3id.org/CEON/ontology/actor/IssuingResource
actor:IssuingResource rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( actorODP:ResourceRelation
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty actorODP:participantRole ;
                                                                   owl:hasValue actor:issuer
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:subClassOf actorODP:ResourceRelation ;
                      rdfs:comment "The relation where the role of the actor is the issuer of a certain resource." ;
                      rdfs:label "Issuing resource" .


###  http://w3id.org/CEON/ontology/actor/OwningResource
actor:OwningResource rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( actorODP:ResourceRelation
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty actorODP:participantRole ;
                                                                  owl:hasValue actor:owner
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf actorODP:ResourceRelation ;
                     rdfs:comment "The relation where the role of the actor is the owner of a certain resource." ;
                     rdfs:label "Owning resource" .


###  http://w3id.org/CEON/ontology/actor/ProcessParticipation
actor:ProcessParticipation rdf:type owl:Class ;
                           rdfs:subClassOf actorODP:CollaborationParticipation ,
                                           [ owl:intersectionOf ( actorODP:CollaborationParticipation
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty actorODP:participantRole ;
                                                                    owl:someValuesFrom actor:ActorCVNRole
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty actorODP:participatingActor ;
                                                                    owl:someValuesFrom actorODP:Actor
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty actorODP:participationIn ;
                                                                    owl:someValuesFrom process:Process
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty actorODP:participationIn ;
